	public void fetchQuestion() {
		try(QuestionDao queDao = new QuestionDaoImp()) {
			List<Question> ques = queDao.getQuestion(13);
			this.queList = ques;
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
