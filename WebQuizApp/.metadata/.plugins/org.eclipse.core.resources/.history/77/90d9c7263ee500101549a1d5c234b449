package com.app.beans;

import java.util.List;

import com.app.daos.QuestionDao;
import com.app.daos.QuestionDaoImp;
import com.app.pojos.Question;

public class QuestionListBean {
	private final int quiz_id;
	private List<Question> queList;
	public QuestionListBean() {
	}
	public List<Question> getQueList() {
		return queList;
	}
	public void setQueList(List<Question> queList) {
		this.queList = queList;
	}
	public void fetchQuestion() {
		try(QuestionDao queDao = new QuestionDaoImp()) {
			this.queList = queDao.getQuestion(this.quiz_id);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
