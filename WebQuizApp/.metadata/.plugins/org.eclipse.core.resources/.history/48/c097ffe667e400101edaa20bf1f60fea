package com.app.daos;

import java.sql.PreparedStatement;

public class StudentDaoImp extends Dao implements StudentDao{
	private PreparedStatement stmtFindByEmail;
	private PreparedStatement stmtFindById;
	private PreparedStatement stmtFindAll;
	private PreparedStatement stmtSave;
	private PreparedStatement stmtUpdate;
	private PreparedStatement stmtDeleteById;
	
	public StudentDaoImp() throws Exception {
		stmtFindByEmail = con.prepareStatement();
		stmtFindById = con.prepareStatement();
		stmtFindAll = con.prepareStatement(null);
		stmtSave = con.prepareStatement(null);
		stmtUpdate = con.prepareStatement(null);
		stmtDeleteById = con.prepareStatement(null);
	}
}
